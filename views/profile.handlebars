<body class="bg-white">
    <header class="bg-gray-600">
      <h1 class="text-3xl p-3 text-white">{{user}}'s Profile</h1>
    </header>

    <div class="p-3 grid place-items-center">
      <div id="username" class="rounded bg-gray-300">Current username: {{user}}</div>

      <div id="email" class="rounded bg-gray-300">Curent email: {{email}}</div>

      <div id="phone" class="rounded bg-gray-300">Current phone number: {{phone}}</div>

      <div id="location" class="rounded bg-gray-300">Current listed location: {{location}}</div>

      <div id="id" class="rounded bg-gray-300">Assigned id: {{id}}</div>
    </div>
</body>